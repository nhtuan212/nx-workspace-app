{
    "name": "nx-workspace-app",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "dev": "yarn backend:dev & yarn frontend:dev",
        "build": "yarn workspaces run build",
        "format": "yarn workspaces run format",
        "lint": "yarn workspaces run lint",
        "lint-staged": "yarn workspaces run lint-staged",
        "backend:dev": "yarn --cwd packages/backend dev",
        "frontend:dev": "yarn --cwd packages/frontend dev",
        "prepare": "husky install"
    },
    "private": true,
    "devDependencies": {
        "@nrwl/nx-cloud": "latest",
        "nx": "15.5.0",
        "prettier": "^2.8.2",
        "husky": "^8.0.0"
    },
    "workspaces": [
        "packages/*"
    ],
    "dependencies": {
        "axios": "^1.3.4",
        "concurrently": "^8.0.1",
        "packages/frontend": "1.0.0"
    }
}