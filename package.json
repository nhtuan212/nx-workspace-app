{
    "name": "nx-workspace-app",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "dev": "concurrently \"yarn backend dev\" \"yarn frontend dev\"",
        "build": "concurrently \"yarn backend build\" \"yarn frontend build\"",
        "format": "concurrently \"yarn backend format\" \"yarn frontend format\"",
        "lint": "concurrently \"yarn backend lint\" \"yarn frontend lint\"",
        "lint-staged": "concurrently \"yarn backend lint-staged\" \"yarn frontend lint-staged\"",
        "backend": "yarn --cwd packages/backend",
        "frontend": "yarn --cwd packages/frontend",
        "prepare": "husky install"
    },
    "private": true,
    "devDependencies": {
        "@nrwl/nx-cloud": "latest",
        "nx": "15.5.0",
        "prettier": "^2.8.2",
        "husky": "^8.0.0"
    },
    "workspaces": [
        "packages/*"
    ],
    "dependencies": {
        "axios": "^1.3.4",
        "concurrently": "^8.0.1",
        "packages/frontend": "1.0.0"
    }
}