{
    "name": "nx-workspace-app",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "dev": "concurrently \"yarn backend-dev\" \"yarn frontend-dev\"",
        "backend-dev": "yarn --cwd packages/backend dev",
        "frontend-dev": "yarn --cwd packages/frontend dev",
        "start": "yarn dev --workspaces",
        "build": "yarn build --workspaces",
        "format": "yarn format --workspaces",
        "lint": "yarn lint --workspaces",
        "prepare": "husky install",
        "lint-staged": "npm run lint-staged --workspaces"
    },
    "private": true,
    "devDependencies": {
        "@nrwl/nx-cloud": "latest",
        "nx": "15.5.0",
        "prettier": "^2.8.2"
    },
    "workspaces": [
        "packages/*"
    ],
    "dependencies": {
        "axios": "^1.3.4",
        "concurrently": "^8.0.1"
    }
}